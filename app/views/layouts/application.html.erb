<!DOCTYPE html>
<html>
  <head>
    <title>MessRoom</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- <link rel="stylesheet" href="https://cdn.simplecss.org/simple.min.css"> -->


     <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"
    integrity="sha512-Fo3rlrZj/k7ujTnHg4CGR2D7kSs0v4LLanw2qksYuRlEzO+tcaEPQogQ0KaoGN26/zrn20ImR1DfuLWnOo7aBA=="
    crossorigin="anonymous"
    referrerpolicy="no-referrer"/>

    
  </head>

  <body>
    <div class="blackdiv hide" id="blckdiv"></div>
    <%= render 'layouts/partials/nav' %>

    <%= render 'layouts/flash' %>
    <div class="container">
    <%= yield %>
      
    </div>

    <%= render 'layouts/partials/search' %>

    <button id="float_search_btn"><i class="fa-solid fa-magnifying-glass"></i></button>

    <%= render 'layouts/partials/footer' %>

    <%= javascript_pack_tag 'myjs' %>
    <%= javascript_pack_tag 'flash' %>
    <%= javascript_pack_tag 'float_search' %>
  </body>
</html>
