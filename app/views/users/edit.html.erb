<div class="header">
	<h1 class="logo_color">Edit your account</h1>
</div>


<%= render 'users/error',model_name:@user  %>
<div class="signup_form_container">
  <div class="signup_form">

  	<% if @owner %>
	    <%= render 'users/form',para:'Update',model_name: @user,owner:@owner,myurl:user_path(@user)%>
	<% else %>
	    <%= render 'users/form',para:'Update',model_name: @user,owner:@owner%>
  		
  	<% end %>
  </div>
</div>
