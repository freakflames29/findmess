
<% if owner %>
  
<%= form_for model_name,url:myurl do |f| %>
  <%= f.label :name ,class:'form_label'%> 
  <%= f.text_field :name,placeholder:'Your name' ,class:'form_field'%> 

  <%= f.label :username ,class:'form_label'%> <em>(minimum is 6 charectors)</em>
  <%= f.text_field :username,placeholder:"Your username" ,required: true,class:'form_field'%> 
   
